{
  "name": "osrs-flipper-pro-backend",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "concurrently \"cross-env NODE_ENV=development node server.js\" \"cross-env REACT_APP_API_BASE=http://localhost:3001 npm --prefix osrs-flipper-pro start\"",
    "prod": "cross-env REACT_APP_API_BASE=https://api.flipper-pro.com npm --prefix osrs-flipper-pro start",
    "prod:log": "cross-env NODE_ENV=production node server.js > logs/prod-output.log 2>&1",
    "prod:test": "node scripts/test-prod-server.js",
    "scheduler": "node poller/scheduler.js"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "better-sqlite3": "^9.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "node-fetch": "^2.7.0",
    "pg": "^8.11.0"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0"
  }
}
